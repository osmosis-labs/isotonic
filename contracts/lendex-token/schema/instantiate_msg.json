{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "InstantiateMsg",
  "type": "object",
  "required": [
    "controller",
    "decimals",
    "name",
    "symbol"
  ],
  "properties": {
    "controller": {
      "description": "Controller is contract allowed to ming, burn, rebase, and must be checked with to enable transfer",
      "type": "string"
    },
    "decimals": {
      "description": "Token precision for displaying",
      "type": "integer",
      "format": "uint8",
      "minimum": 0.0
    },
    "name": {
      "description": "Token name",
      "type": "string"
    },
    "symbol": {
      "description": "Token symbol",
      "type": "string"
    }
  }
}
