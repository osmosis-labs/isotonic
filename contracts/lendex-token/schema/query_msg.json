{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "QueryMsg",
  "oneOf": [
    {
      "description": "Returns the current balance of the given address, 0 if unset. Return type: `BalanceResponse`.",
      "type": "object",
      "required": [
        "balance"
      ],
      "properties": {
        "balance": {
          "type": "object",
          "required": [
            "address"
          ],
          "properties": {
            "address": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Returns metadata on the contract - name, decimals, supply, etc. Return type: `TokenInfoResponse`.",
      "type": "object",
      "required": [
        "token_info"
      ],
      "properties": {
        "token_info": {
          "type": "object"
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Returns the global multiplier factor.",
      "type": "object",
      "required": [
        "multiplier"
      ],
      "properties": {
        "multiplier": {
          "type": "object"
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Funds distributed by this contract. Returns `FundsResponse`.",
      "type": "object",
      "required": [
        "distributed_funds"
      ],
      "properties": {
        "distributed_funds": {
          "type": "object"
        }
      },
      "additionalProperties": false
    },
    {
      "description": "FUnds send to this contact but not yet distributed. Returns `FundsResponse`.",
      "type": "object",
      "required": [
        "undistributed_funds"
      ],
      "properties": {
        "undistributed_funds": {
          "type": "object"
        }
      },
      "additionalProperties": false
    },
    {
      "description": "Queries for funds distributed but not yet withdrawn by owner",
      "type": "object",
      "required": [
        "withdrawable_funds"
      ],
      "properties": {
        "withdrawable_funds": {
          "type": "object",
          "required": [
            "owner"
          ],
          "properties": {
            "owner": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    }
  ]
}
